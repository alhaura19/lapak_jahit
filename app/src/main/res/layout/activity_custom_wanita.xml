            <TextView
                android:id="@+id/text_fw_gaun"
                android:layout_width="match_parent"
                android:layout_below="@id/ic_fw_gaun"
                android:layout_height="@dimen/custom_textview_height"
                android:text="@string/fw_gaun"
                android:layout_weight="0.33"
                android:textColor="@android:color/black"
                android:textSize="@dimen/size_text_secondary"
                android:textAlignment="center"/>
        </LinearLayout>
        <LinearLayout
        android:id="@+id/customwanita_dress"
        android:layout_width="@dimen/custom_linearlayout_width"
        android:layout_height="wrap_content"
        android:orientation="vertical">
